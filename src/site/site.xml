<project xmlns="http://maven.apache.org/DECORATION/1.4.0"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xsi:schemaLocation="http://maven.apache.org/DECORATION/1.4.0 http://maven.apache.org/xsd/decoration-1.4.0.xsd"
			name="${project.name}">
	<publishDate position="right" />
	<version position="right" />
	<bannerLeft>
		<name>${project.name}</name>
		<src>images/logos/RunDeckMonitor.png</src>
		<href>index.html</href>
	</bannerLeft>
	<skin>
		<groupId>org.apache.maven.skins</groupId>
		<artifactId>maven-fluido-skin</artifactId>
		<version>1.3.1</version>
	</skin>
	<custom>
		<fluidoSkin>
		  <gitHub>
			<projectId>Sylvain-Bugat/RundeckMonitor</projectId>
			<ribbonOrientation>right</ribbonOrientation>
			<ribbonColor>red</ribbonColor>
			<sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
			<breadcrumbDivider>-></breadcrumbDivider>
		  </gitHub>
		</fluidoSkin>
	</custom>
	<poweredBy>
		<logo name="Maven" href="http://maven.apache.org/" img="images/logos/maven-feather.png"/>
		<logo name="GitHub" href="https://github.com/" img="images/logos/GitHub.png"/>
	</poweredBy>
	<body>
		<breadcrumbs>
			<item name="Rundeck" href="http://rundeck.org" />
			<item name="Rundeck Monitor" href="" />
		</breadcrumbs>
		<menu name="${project.name}">
			<item name="Introduction" href="index.html" />
			<item name="Download and install" href="download-install.html" />
			<item name="GitHub readme" href="README.html" />
			<item name="FAQ" href="faq.html" />
			<item name="Changelog" href="changes-report.html" />
		</menu>
		<menu ref="reports" />
	</body>
</project>
